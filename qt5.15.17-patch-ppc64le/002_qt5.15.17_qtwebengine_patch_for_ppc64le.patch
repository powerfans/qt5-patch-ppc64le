+  void CmplSmiLiteral(Register src1, Smi smi, Register scratch,
+                      CRegister cr = cr7);
+  void AndSmiLiteral(Register dst, Register src, Smi smi, Register scratch,
+                     RCBit rc = LeaveRC);
