+void Assembler::UntrackBranch() {
+  DCHECK(!trampoline_emitted_);
+  DCHECK_GT(tracked_branch_count_, 0);
+  int count = --tracked_branch_count_;
+  if (count == 0) {
+    // Reset
+    next_trampoline_check_ = kMaxInt;
+  } else {
+    next_trampoline_check_ += kTrampolineSlotsSize;
+  }
+}
