+  } else {
+    if (is_uint16(rb.immediate()) && RelocInfo::IsNone(rb.rmode_) &&
+        rc == LeaveRC) {
+      xori(ra, rs, rb);
+    } else {
+      // mov handles the relocation.
+      DCHECK(rs != r0);
+      mov(r0, rb);
+      xor_(ra, rs, r0, rc);
+    }
+  }
+}
