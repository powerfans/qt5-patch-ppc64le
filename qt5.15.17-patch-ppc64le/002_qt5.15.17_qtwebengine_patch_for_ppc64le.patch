+void Assembler::b(int branch_offset, LKBit lk) {
+  int imm26 = branch_offset;
+  CHECK(is_int26(imm26) && (imm26 & (kAAMask | kLKMask)) == 0);
+  emit(BX | (imm26 & kImm26Mask) | lk);
+}
