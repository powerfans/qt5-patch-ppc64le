+void Assembler::bc(int branch_offset, BOfield bo, int condition_bit, LKBit lk) {
+  int imm16 = branch_offset;
+  CHECK(is_int16(imm16) && (imm16 & (kAAMask | kLKMask)) == 0);
+  emit(BCX | bo | condition_bit * B16 | (imm16 & kImm16Mask) | lk);
+}
