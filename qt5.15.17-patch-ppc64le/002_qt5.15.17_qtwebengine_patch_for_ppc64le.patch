+void Assembler::Align(int m) {
+  DCHECK(m >= 4 && base::bits::IsPowerOfTwo(m));
+  DCHECK_EQ(pc_offset() & (kInstrSize - 1), 0);
+  while ((pc_offset() & (m - 1)) != 0) {
+    nop();
+  }
+}
